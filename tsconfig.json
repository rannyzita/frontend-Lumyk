{
  "compilerOptions": {
    "jsx": "react-native", 
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "lib": ["esnext"],
    "types": ["react", "react-native"]
  }
}
